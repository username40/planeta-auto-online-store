{
  "private": true,
  "scripts": {
    "dev:site": "nuxt -c client_apps/site/nuxt.config.js --port 4444",
    "dev:manage": "nuxt -c client_apps/manage/nuxt.config.js --port 3333",
    "dev": "concurrently \"npm run dev:site\" \"npm run dev:manage\" \"npm run placeholder\"",
    "build:site": "nuxt build -c client_apps/site/nuxt.config.js",
    "build:manage": "nuxt build -c client_apps/manage/nuxt.config.js",
    "build": "concurrently \"npm run build:site\" \"npm run build:manage\"",
    "start:site": "nuxt start -c client_apps/site/nuxt.config.js --port 4444",
    "start:manage": "nuxt start -c client_apps/manage/nuxt.config.js --port 3333",
    "start": "concurrently \"npm run start:site\" \"npm run start:manage\"",
    "placeholder": "node placeholder"
  },
